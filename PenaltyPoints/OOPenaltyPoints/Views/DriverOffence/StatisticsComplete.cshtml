@model string

@{
    ViewBag.Title = "StatisticsComplete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>The Driver Offence Statistics You Requested To View Are:</h2>

@if(string.IsNullOrWhiteSpace(ViewBag.MultilineRaw) == false)
{
    <ul>
        @foreach(var line in @ViewBag.MultilineSplitted)
        {
        <li>@line</li>
        }
    </ul>
}

<p>
    @Html.ActionLink("New Statistics", "Statistics", "DriverOffence")
</p>
