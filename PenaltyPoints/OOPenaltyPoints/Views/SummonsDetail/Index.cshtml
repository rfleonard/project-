@model IEnumerable<OOPenaltyPoints.Models.SummonsDetail>

@{
    ViewBag.Title = "List";
}

<h2>Index Of Court Summons Notifications Sent</h2>


<table>
    <tr>
        <th>
            Date issued:
        </th>
        <th>
            Judgement:
        </th>
        <th>
           First Name:
        </th>
        <th>
            Surname:
        </th>
        <th>
           Address:
        </th>
        <th>
            Town:
        </th>
        <th>
           County:
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SdDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SdJudgement)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SdFName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SdSName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SdAddress1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SdAddress2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SdAddress3)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.Id }) 

        </td>
    </tr>
}

</table>
